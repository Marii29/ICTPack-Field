<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.marii.signup">

    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.SEND_SMS" />
    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".WelcomeScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".YourOrder" />
        <activity android:name=".Billing" />
        <activity android:name=".Reservations" />
        <activity android:name=".Delivery" />
        <activity android:name=".Send_friend_money" />
        <activity android:name=".Top_up_account" />
        <activity android:name=".Invite_Friends" />
        <activity android:name=".Home" />
        <activity android:name=".Settings" />
        <activity android:name=".MainActivity" />
        <activity android:name=".UserProfile" />
    </application>

</manifest>